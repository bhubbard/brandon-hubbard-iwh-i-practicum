html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="preconnect", href="https://cdn.jsdelivr.net")
        link(rel="preconnect", href="https://fonts.googleapis.com")
        link(rel="preconnect", href="ttps://fonts.gstatic.com" crossorigin)
        link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css")
        link(rel="stylesheet", href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap")
        link(rel="stylesheet", href="/css/style.css")
        script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js")
    body
        div.container
            h1.ml-0.pl-0 Add New Car
            p
                a(href="/") Return to the homepage
            form(method="POST")
                label(for="car_name") Car Name
                input#name(type="text" name="car_name" required)
                label(for="car_year") Car Year
                input#artist(type="year" name="car_year" required)
                label(for="car_type") Car Type
                input#label(type="text" name="car_type" required)
                input(type="submit" value="Submit")